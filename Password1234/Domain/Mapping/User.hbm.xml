<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="User" table="`user`">
    <id name="IdU" column="id_user" type="int">
      <generator class="native"></generator>
    </id>
    <property name="PasswordU" column="password_user" not-null="true"/>
    <property name="LoginU" column="login_user" not-null="true"/>
  </class>

</hibernate-mapping>